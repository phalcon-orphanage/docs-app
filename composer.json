{
    "name": "phalcon/docs",
    "description": "Official Phalcon Documentation Website",
    "keywords": [
        "documentation",
        "phalcon",
        "framework",
        "extension"
    ],
    "type": "project",
    "license": "BSD-3-Clause",
    "homepage": "https://docs.phalconphp.com/",
    "authors": [
        {
            "name": "Phalcon Team",
            "email": "team@phalconphp.com",
            "homepage": "https://phalconphp.com/en/team"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/phalcon/docs-app/graphs/contributors"
        }
    ],
    "support": {
        "source": "https://github.com/phalcon/docs-app",
        "issues": "https://github.com/phalcon/docs-app/issues",
        "forum": "https://forum.phalconphp.com/",
        "docs": "https://docs.phalconphp.com"
    },
    "autoload": {
        "psr-4": {
            "Docs\\": "app/library/",
            "Docs\\Providers\\": "app/providers/",
            "Docs\\Cli\\Tasks\\": "app/tasks/",
            "Docs\\Controllers\\": "app/controllers/"
        },
        "files": [
            "bootstrap/functions.php"
        ]
    },
    "require": {
        "php": ">= 7.0",
        "ext-json": "*",
        "ext-phalcon": "~3.2",
        "dariuszp/cli-progress-bar": "^1.0",
        "erusev/parsedown": "1.7.*",
        "erusev/parsedown-extra": "0.7.*",
        "filp/whoops": "2.1.*",
        "guzzlehttp/psr7": "1.5.*",
        "league/flysystem": "1.0.*",
        "vlucas/phpdotenv": "2.4.*"
    },
    "require-dev": {
        "codeception/codeception": "2.4.*",
        "codeception/mockery-module": "0.2.*",
        "codeception/specify": "0.4.*",
        "codeception/verify": "0.4.*",
        "doctrine/instantiator": "1.0.5",
        "phalcon/dd": "^1.1",
        "phpdocumentor/reflection-docblock": "^2.0",
        "phpmd/phpmd": "2.6.*",
        "phpunit/phpunit": "6.4.*",
        "squizlabs/php_codesniffer": "3.2.*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
